
<button class="icon-plus">New Entry</button>

<table class="river-card-log-entries">
  <thead>
    <tr>
      <th colspan="3" class="log-year">2014</th>
    </tr>
    <tr>
      <th>Date</th>
      <th>Notes</th>
    </tr>
  </thead>
  <tbody>
    {{#each userData.log}}
    <tr>
      <td>
        <div class="date-cell">
          {{date}}
          <div class="log-actions">
            <a class="icon-link icon-pencil"></a>
            <a class="icon-link icon-remove"></a>
          </div>
        </div>
      </td>
      <td class="notes"> {{notes}} </td>
    </tr>
    {{/each}}
  </tbody>
</table>
